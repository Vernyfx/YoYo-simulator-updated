local Main = Window:MakeTab({
	Name = "Auto Eggs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

--worlds

function teleportTO(placeCFrame)
local plyr = game.Players.LocalPlayer
if plyr.Character then
plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
   end
end
function teleportWorld(world)
  if game:GetService("Workspace").MapFunctions.Zones:FindFirstChild(world) then
    teleportTO(game:GetService("Workspace").MapFunctions.Zones[world].CFrame)
  end
end

teleportWorld('Toyland')

teleportTO(game:GetService("Workspace").MapFunctions.Zones.Toyland.CFrame)





--eggs


local selectedEgg;

Main:AddDropdown({
	Name = "Select Egg To tp",
	Default = "1",
	Options = {"Starter Egg", "Premium Egg", "Ice Cream Egg", "Candy Cane Egg", "Cactus Egg", "Mummy Egg", "Sand Egg", "Hydra Egg", "Lava Rock Egg", "Volcano Egg", "Lava Dragon Egg", "Neptune Egg", "Cthulu Egg", "Trident Egg", "Watermelon Egg", "Burger Egg", "Actual Egg", "8-Bit Egg", "Toy Egg", "Gold 8-Bit Egg", "Rare Egg",},
	Callback = function(Eggtp)
		selectedEgg = Eggtp;
    print(Eggtp)
	end    
})

--[[
Name = <string> - The name of the dropdown.
Default = <string> - The default value of the dropdown.
Options = <table> - The options in the dropdown.
Callback = <function> - The function of the dropdown.
]]


Main:AddButton({
	Name = "Teleport to selected egg",
	Callback = function()
if selectedEgg then
      		teleportEgg(selectedEgg)
end
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]






function teleportTO2(placeCFrame)
  local plyr = game.Players.LocalPlayer
  if plyr.Character then
  plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
     end
    end
  function teleportEgg(egg)
    if game:GetService("Workspace").MapFunctions.Eggs:FindFirstChild(egg) then
      teleportTO2(game:GetService("Workspace").MapFunctions.Eggs[egg].EggInfo.PricePart.CFrame)
    end
  end
  
teleportEgg('Rare Egg')


