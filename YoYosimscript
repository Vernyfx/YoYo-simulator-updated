Main:AddToggle({
	Name = "AutoStarterEgg",
	Default = false,
	Callback = function(Value)
		_G.autoegg1 = Value;

task.spawn(function()
  while _G.autoegg1 do
local args = {
  [1] = "Starter Egg",
  [2] = "SingleEgg",
  [3] = game:GetService("Players").LocalPlayer
}
game:GetService("ReplicatedStorage").GameEvents.EggEvent:FireServer(unpack(args))
wait()
   end
end)
	end    
})

