local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "VernyHub", HidePremium = false, SaveConfig = true, ConfigFolder = "VernyHubOrion", Icon = "https://cdn.discordapp.com/attachments/1014982535140360303/1016417617696784494/unknown.png"})

--[[
Name = "VernyHub" - The name of the UI.
HidePremium = <bool> - Whether or not the user details shows Premium status or not.
SaveConfig = <bool> - Toggles the config saving in the UI.
ConfigFolder = <string> - The name of the folder where the configs are saved.
IntroEnabled = <bool> - Whether or not to show the intro animation.
IntroText = <string> - Text to show in the intro animation.
IntroIcon = <string> - URL to the image you want to use in the intro animation.
Icon = "https://cdn.epicstream.com/assets/uploads/newscover/1626801804destructo.jpg" - URL to the image you want displayed on the window.
CloseCallback = <function> - Function to execute when the window is closed.
]]

local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = "AutoFarm" - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

function teleportTO2(placeCFrame)
  local plyr = game.Players.LocalPlayer
  if plyr.Character then
  plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
     end
  end

  function teleportEgg(egg)
    if game:GetService("Workspace").MapFunctions.Eggs:FindFirstChild(egg) then
      teleportTO2(game:GetService("Workspace").MapFunctions.Eggs[egg].EggInfo.PricePart.CFrame)
    end
  end




local MainSection = Main:AddSection({
	Name = "AutoFarm"
})

Main:AddLabel("I suggest use autoyoyo and auto sell at the same time(OP)")

Main:AddLabel("! DO NOT BUY A NEW YOYO OR BACKPACK WHILE USING")

Main:AddLabel("! AUTO SELL AND AUTO YOYO IT WILL BREAK IT!")

Main:AddToggle({
	Name = "AutoYoyo",
	Default = false,
	Callback = function(Value)
		_G.autoyoyo = Value;

task.spawn(function()
    while wait() do
       if _G.autoyoyo == true then
        game:GetService("ReplicatedStorage").GameEvents.ToolEvents.ToolEvent:FireServer("Merman Yo-Yo",game:GetService("Players").LocalPlayer)
      end
    end
  end) 
end    
})

Main:AddToggle({
	Name = "AutoSell",
	Default = false,
	Callback = function(AutoSV)
		_G.autosell = AutoSV;



task.spawn(function()
    while _G.autosell == true do
        local playerHead = game.Players.LocalPlayer.Character.Head
   for i, v in pairs(game:GetService("Workspace").MapFunctions.Sell.Toyland.HitBox:GetDescendants()) do
      if v.Name == "TouchInterest" and v.Parent then
        firetouchinterest(playerHead, v.Parent, 0)
          wait(0.1)
       firetouchinterest(playerHead, v.Parent, 1)
       break;
    end
end
    end
end)
	end    
})

--[[
Name = <string> - The name of the toggle.
Default = <bool> - The default value of the toggle.
Callback = <function> - The function of the toggle.
]]

_G.CollectChests = pcall(function()
  local playerHead = game.Players.LocalPlayer.Character.Head
  for i, v in pairs(game:GetService("Workspace").MapFunctions.Chests:GetDescendants()) do
      if v.Name == "TouchInterest" and v.Parent then
          firetouchinterest(playerHead, v.Parent, 0)
          wait(0.1)
          firetouchinterest(playerHead, v.Parent, 1)
      end
  end
end)    


Main:AddButton({
  Name = "AutoCollectChests",
  Callback = function()
      _G.CollectChests()
  end    
})  


local Main = Window:MakeTab({
	Name = "RemovePopUps",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

Main:AddLabel("I highly suggest u turn these on since it wont lag ur game")

--removegui

Main:AddToggle({
	Name = "RemoveClicksPopUp",
	Default = false,
	Callback = function(Value)
		_G.removeclicks = Value;

task.spawn(function()
  while _G.removeclicks do
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Clicks.Icon.Visible = false
      wait()
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Clicks.Amount.Visible = false
   if _G.removecoins == false then
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Clicks.Icon.Visible = true
      wait()
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Clicks.Amount.Visible = true
    end

  end

end)
	end    
})

--[[
Name = <string> - The name of the toggle.
Default = <bool> - The default value of the toggle.
Callback = <function> - The function of the toggle.
]]


Main:AddToggle({
	Name = "RemoveCoinsPopUp",
	Default = false,
	Callback = function(Value)
		_G.removecoins = Value;

task.spawn(function()
  while _G.removecoins do
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Coins.Icon.Visible = false
      wait()
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Coins.Amount.Visible = false
   if _G.removecoins == false then
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Coins.Icon.Visible = true
      wait()
      game:GetService("Players").LocalPlayer.PlayerGui.MainGui.PopUpFrame.Coins.Amount.Visible = true
    end

  end

end)
	end    
})

--[[
Name = <string> - The name of the toggle.
